{
  "name": "rexsoftware/array-object",
  "description": "Provides an interface for simplifying working with associative and sequential arrays (collections).",
  "keywords": [
    "array",
    "arrays",
    "interface",
    "fluent",
    "collection",
    "collections",
    "queue",
    "stack",
    "iterator",
    "magic"
  ],
  "type": "library",
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0",
    "bkvfoundry/utility-belt": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "RexSoftware\\ArrayObject\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RexSoftware\\ArrayObject\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "tests": "phpunit",
    "coverage": "phpunit --coverage-html ./report"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jodie Dunlop",
      "email": "jodie.dunlop@rexsoftware.com.au",
      "role": "Developer"
    }
  ]
}
